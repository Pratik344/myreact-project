if (!formIsOk) {\n      return alert('Please do not leave empty fields');\n    }\n\n    // Validate email\n    if (!this.validateEmail(this.state.email)) {\n      alert('Invalid email, please enter email format as \"myEmail@mydomain.com');\n      return;\n    }\n\n    // Validate phone number\n    if (!this.validatePhoneNumber(this.state.phone_number)) {\n      alert('Invalid phone number format, please follow format: 888-888-8888');\n      return;\n    }\n\n    // Validate url string\n    if (!this.validateImageUrl(this.state.image_url)) {\n      alert('Invalid image url')\n      return;\n    }\n\n    sendEvent('addNewContact', this.state);\n    this.props.history.push('/mycontact/contact');\n\n  }\n\n  render() {\n\n    return (\n      <div className=\"container my\">\n        <Link to=\"/mycontact/contact\">Go Back</Link>\n        <Form className=\"contact-form\">\n          <h1 className=\"contact-header\">Add new contact</h1>\n          <div className=\"form-group\">\n            <input\n              className=\"form-control\"\n              onChange={this.onInputChange}\n              value={this.state.name}\n              name=\"name\"\n              placeholder='name'\n            />\n          </div>\n\n          <div className=\"form-group\">\n            <input\n              type=\"email\"\n              className=\"form-control\"\n              value={this.state.email}\n              aria-describedby=\"emailHelp\"\n              onChange={this.onInputChange}\n              name=\"email\"\n              placeholder='email'\n            />\n          </div>\n\n          <div className=\"form-group\">\n            <input\n              className=\"form-control\"\n              value={this.state.phoneNumber}\n              onChange={this.onInputChange}\n              name=\"phone_number\"\n              placeholder='Enter phone number'\n            />\n          </div>\n\n          <div className=\"form-group\">\n            <input\n              className=\"form-control\"\n              value={this.state.imageUrl}\n              onChange={this.onInputChange}\n              name=\"image_url\"\n              placeholder='Image URL'\n            />\n          </div>\n          <button className=\"btn btn-info\" type='submit' onClick={this.handleSubmit}>Submit</button>\n        </Form>\n      </div>\n    )\n  }\n}\n","C:\\Pratik\\React JS\\mycontact\\src\\components\\Contact.js",[],"C:\\Pratik\\React JS\\mycontact\\src\\components\\NavBar.js",[],{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","severity":1,"message":"44","line":28,"column":46,"nodeType":"45","messageId":"46","endLine":28,"endColumn":47,"suggestions":"47"},{"ruleId":"43","severity":1,"message":"44","line":28,"column":71,"nodeType":"45","messageId":"46","endLine":28,"endColumn":72,"suggestions":"48"},{"ruleId":"43","severity":1,"message":"44","line":28,"column":80,"nodeType":"45","messageId":"46","endLine":28,"endColumn":81,"suggestions":"49"},{"ruleId":"43","severity":1,"message":"44","line":28,"column":84,"nodeType":"45","messageId":"46","endLine":28,"endColumn":85,"suggestions":"50"},{"ruleId":"43","severity":1,"message":"51","line":34,"column":51,"nodeType":"45","messageId":"46","endLine":34,"endColumn":52,"suggestions":"52"},"no-native-reassign",["53"],"no-negated-in-lhs",["54"],"no-useless-escape","Unnecessary escape character: \\\".","Literal","unnecessaryEscape",["55","56"],["57","58"],["59","60"],["61","62"],"Unnecessary escape character: \\-.",["63","64"],"no-global-assign","no-unsafe-negation",{"messageId":"65","fix":"66","desc":"67"},{"messageId":"68","fix":"69","desc":"70"},{"messageId":"65","fix":"71","desc":"67"},{"messageId":"68","fix":"72","desc":"70"},{"messageId":"65","fix":"73","desc":"67"},{"messageId":"68","fix":"74","desc":"70"},{"messageId":"65","fix":"75","desc":"67"},{"messageId":"68","fix":"76","desc":"70"},{"messageId":"65","fix":"77","desc":"67"},{"messageId":"68","fix":"78","desc":"70"},"remo